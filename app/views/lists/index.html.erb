<div class= "container">
<h1>Display Lists</h1>
<ul>
   <% @lists.each do |list| %>
    <li><%= link_to list.name, list_path(list) %></li>

  <% end %>
  <%= cl_image_tag("THE_IMAGE_ID_FROM_LIBRARY",
      width: 400, height: 300, crop: :fill) %>

<%= form_for @user, url: user_path(@user), method: :patch, html: { multipart: true } do |f| %>
  <%= f.file_field :profile_picture %>
  <%= f.submit "Update Profile" %>
<% end %>


</ul>
<p><%= link_to "Add New List", new_list_path, class:"btn btn-primary"  %></p>
<%# <p><%= link_to "Add New List", new_list_path, class:"btn btn-danger"  %></p>
</div>
